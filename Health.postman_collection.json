		{
			"name": "Retrive user information",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"\r",
							"pm.test(\"Unauthorized code\", function () {\r",
							"    pm.response.to.have.status(401);\r",
							"});\r",
							"\r",
							"pm.test(\"Response body contains 'success' property\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData).to.have.property('success');\r",
							"});\r",
							"\r",
							"pm.test(\"'success' property is a boolean\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.success).to.be.a('boolean');\r",
							"});\r",
							"\r",
							"pm.test(\"'status' property is present\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData).to.have.property('status');\r",
							"});\r",
							"\r",
							"pm.test(\"'status' property is a number\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.status).to.be.a('number');\r",
							"});\r",
							"\r",
							"pm.test(\"'message' property is present\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData).to.have.property('message');\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
